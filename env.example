# Healthcare WhatsApp Bot Environment Variables

# Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API (for speech-to-text)
OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face API (for Llama 3.1 multilingual responses via Featherless AI)
HF_TOKEN=your_huggingface_token_here

# AI Model Selection (gemini, llama, hybrid)
AI_MODEL_PROVIDER=hybrid

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# WhatsApp
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token_here
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id_here
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your_webhook_verify_token_here

# Server
PORT=3000
NODE_ENV=production
